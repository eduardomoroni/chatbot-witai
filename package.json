{
  "name": "chatbot-witai",
  "version": "0.0.1",
  "author": "eduardomoroni",
  "repository": {
    "type": "git",
    "url": "git@github.com:eduardomoroni/chatbot-witai.git"
  },
  "scripts": {
    "start": "npm run hubot",
    "docker:start": "sh src/scripts/docker.sh",
    "lint:check": "eslint ./src/ ./tests/",
